{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "jsx-a11y/label-has-for": "off",
    "react/require-default-props": 0
  },
  "settings": {
    "propWrapperFunctions": ["forbidExtraProps", "exact", "Object.freeze"]
  }
}
